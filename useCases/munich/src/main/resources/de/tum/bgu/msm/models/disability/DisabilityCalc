var calculateDisabilityProbability = function(personAge, personSex) {

 alpha = 0.;

    if (personSex == 1) {
    // sex = male
        if (personAge < 4){
            alpha = 0.001381;
        } else if (personAge < 6){
            alpha = 0.0030895;
        } else if (personAge < 15){
            alpha = 0.0007122;
        } else if (personAge < 18){
            alpha = 0.000535;
        } else if (personAge < 25){
            alpha = 0.0002874;
        } else if (personAge < 35){
            alpha = 0.0004203;
        } else if (personAge < 45){
            alpha = 0.0010992;
        } else if (personAge < 55){
            alpha = 0.0032488;
        } else if (personAge < 60){
            alpha = 0.0120512;
        } else if (personAge < 62){
            alpha = 0.0256974;
        } else if (personAge < 65){
            alpha = 0.013692;
        } else if (personAge < 70){
            alpha = 0.0040253;
        } else if (personAge < 75){
            alpha = 0.0003487;
        } else {
            alpha = 0.00504426;
        }
    } else if (personSex == 2) {
    // sex = female
        if (personAge < 4){
            alpha = 0.0011646;
        } else if (personAge < 6){
            alpha = 0.0020148;
        } else if (personAge < 15){
            alpha = 0.0004008;
        } else if (personAge < 18){
            alpha = 0.0006072;
        } else if (personAge < 25){
            alpha = 0.0003531;
        } else if (personAge < 35){
            alpha = 0.0004519;
        } else if (personAge < 45){
            alpha = 0.0013524;
        } else if (personAge < 55){
            alpha = 0.0033078;
        } else if (personAge < 60){
            alpha = 0.0096176;
        } else if (personAge < 62){
            alpha = 0.0190294;
        } else if (personAge < 65){
            alpha = 0.0056981;
        } else if (personAge < 70){
            alpha = 0.0006603;
        } else if (personAge < 75){
            alpha = 0.0005000;
        } else {
            alpha = 0.00609682;
        }
    }

    return alpha;
}

var probabilityForPhysicalDisability = function(personAge, personSex) {

 alpha = 0.;

    if (personSex == 1) {
    // sex = male
        if (personAge < 4){
            alpha = 0.5449;
        } else if (personAge < 6){
            alpha = 0.4700;
        } else if (personAge < 15){
            alpha = 0.4040;
        } else if (personAge < 18){
            alpha = 0.4001;
        } else if (personAge < 25){
            alpha = 0.3744;
        } else if (personAge < 35){
            alpha = 0.3890;
        } else if (personAge < 45){
            alpha = 0.4773;
        } else if (personAge < 55){
            alpha = 0.5817;
        } else if (personAge < 60){
            alpha = 0.6790;
        } else if (personAge < 62){
            alpha = 0.7196;
        } else if (personAge < 65){
            alpha = 0.7456;
        } else if (personAge < 70){
            alpha = 0.7766;
        } else if (personAge < 75){
            alpha = 0.7965;
        } else {
            alpha = 0.8039;
        }
    } else if (personSex == 2) {
    // sex = female
        if (personAge < 4){
            alpha = 0.5370;
        } else if (personAge < 6){
            alpha = 0.5091;
        } else if (personAge < 15){
            alpha = 0.4644;
        } else if (personAge < 18){
            alpha = 0.4571;
        } else if (personAge < 25){
            alpha = 0.4091;
        } else if (personAge < 35){
            alpha = 0.4402;
        } else if (personAge < 45){
            alpha = 0.5325;
        } else if (personAge < 55){
            alpha = 0.6059;
        } else if (personAge < 60){
            alpha = 0.6655;
        } else if (personAge < 62){
            alpha = 0.6942;
        } else if (personAge < 65){
            alpha = 0.7161;
        } else if (personAge < 70){
            alpha = 0.7526;
        } else if (personAge < 75){
            alpha = 0.7847;
        } else {
            alpha = 0.7997;
        }
    }

    return alpha;
}


var calculateBaseYearDisabilityProbability = function(personAge, personSex) {
alpha = 0.;

    if (personSex == 1) {
    // sex = male
        if (personAge < 4){
            alpha = 0.00552;
        } else if (personAge < 6){
            alpha = 0.0117;
        } else if (personAge < 15){
            alpha = 0.0181;
        } else if (personAge < 18){
            alpha = 0.0197;
        } else if (personAge < 25){
            alpha = 0.0217;
        } else if (personAge < 35){
            alpha = 0.0259;
