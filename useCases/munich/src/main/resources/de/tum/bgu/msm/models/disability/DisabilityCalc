            alpha = 0.0694;
        } else if (personAge < 60){
            alpha = 0.1297;
        } else if (personAge < 62){
            alpha = 0.1811;
        } else if (personAge < 65){
            alpha = 0.2221;
        } else if (personAge < 70){
            alpha = 0.2423;
        } else if (personAge < 75){
            alpha = 0.2440;
        } else {
            alpha = 0.3197;
        }
    } else if (personSex == 2) {
    // sex = female
        if (personAge < 4){
            alpha = 0.0047;
        } else if (personAge < 6){
            alpha = 0.0087;
        } else if (personAge < 15){
            alpha = 0.0123;
        } else if (personAge < 18){
            alpha = 0.0141;
        } else if (personAge < 25){
            alpha = 0.0166;
        } else if (personAge < 35){
            alpha = 0.0211;
